<% include __header %>
<h1 align="center">Sign up</h1>

<form action="/signup" method="post">

    <input name="username" type="text" maxlength="25" class="form-control" placeholder="Username" required autofocus>

    <input name="password" type="password" maxlength="16" class="form-control" placeholder="Password" required>

    <input name="first_name" type="text" maxlength="40" class="form-control" placeholder="First name" required>

    <input name="last_name" type="text" maxlength="40" class="form-control" placeholder="Last name" required>

    <input type="submit" value="Sign up" class="btn btn-primary btn-block">

</form>
<% include __footer %>